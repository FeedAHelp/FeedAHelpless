{ "include": ["./**/*.ts"],
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "rootDir": "./",
    "outDir": "dist",
    "baseUrl": "./apps/feedahelp-server",
    "paths": {
      "*": ["node_modules/*", "src/types/*", "src/config/*"]
    }
  },
  
}
